{
  "name": "elv-components-js",
  "version": "1.0.0",
  "description": " React components and SCSS styling for Eluvio applications ",
  "main": "src/index.js",
  "scripts": {
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint-scss": "./node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "./node_modules/sass-lint-auto-fix/dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eluv-io/elv-components-js.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/eluv-io/elv-components-js/issues"
  },
  "homepage": "https://github.com/eluv-io/elv-components-js#readme",
  "dependencies": {
    "luxon": "^1.24.1",
    "moment-timezone": "^0.5.31",
    "path": "^0.12.7",
    "pretty-bytes": "^5.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.8.6",
    "react-inlinesvg": "^1.1.5",
    "react-router-dom": "^5.1.2",
    "react-truncate": "^2.4.0",
    "resize-observer-polyfill": "^1.5.1",
    "select": "^1.1.2",
    "urijs": "^1.19.2",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-plugin-react": "^7.12.4",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.17.0"
  }
}
